        android:textSize="24sp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="C0771910"
        android:textAlignment="center"
        android:textSize="24sp" />
</LinearLayout>
